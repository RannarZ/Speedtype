<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.TextFlow?>

<AnchorPane fx:id="anchor" onKeyPressed="#keyPressed" onKeyReleased="#keyReleased" prefHeight="700.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.speedtype.ChosenSpeedtypeController">
    <children>
        <VBox layoutX="150.0" prefHeight="700.0" prefWidth="400.0" spacing="10.0">
            <children>
                <HBox alignment="TOP_CENTER" prefHeight="60.0" prefWidth="400.0">
                    <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Speedtype chosen text" textAlignment="CENTER">
                            <font>
                                <Font size="40.0" />
                            </font>
                        </Text>
                    </children>
                </HBox>
                <TextField fx:id="textBox" onKeyPressed="#keyPressed" prefWidth="400.0">
                    <VBox.margin>
                        <Insets />
                    </VBox.margin>
                </TextField>
                <TextFlow blendMode="SRC_ATOP" prefWidth="200.0">
                    <children>
                        <Text fx:id="baseText" strokeType="OUTSIDE" strokeWidth="0.0" text="Example text" textAlignment="JUSTIFY" wrappingWidth="400.0" />
                    </children>
                </TextFlow>
                <Button mnemonicParsing="false" onMouseClicked="#returnToMain" text="Return" />
            </children>
            <padding>
                <Insets top="50.0" />
            </padding>
        </VBox>
    </children>
</AnchorPane>
