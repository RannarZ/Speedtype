<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="700.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.speedtype.LeaderboardsController">
   <children>
      <VBox layoutX="150.0" prefHeight="700.0" prefWidth="400.0">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <Button mnemonicParsing="false" text="Button" />
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Full texts">
                     <font>
                        <Font size="53.0" />
                     </font>
                  </Text>
                  <Button mnemonicParsing="false" text="Button" />
               </children>
            </HBox>
            <TableView prefHeight="500.0" prefWidth="400.0">
              <columns>
                <TableColumn minWidth="80.0" text="Nr" />
                <TableColumn minWidth="80.0" text="Name" />
                  <TableColumn minWidth="80.0" text="Text" />
                  <TableColumn minWidth="80.0" text="WPM" />
                  <TableColumn minWidth="80.0" text="Date" />
              </columns>
            </TableView>
            <Button mnemonicParsing="false" onMouseClicked="#returnToMain" text="Return" />
         </children>
      </VBox>
   </children>
</AnchorPane>
