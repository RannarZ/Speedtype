<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="700.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.speedtype.ModesController">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="100.0" prefHeight="700.0" prefWidth="500.0" spacing="50.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Choose typing mode" textAlignment="CENTER">
               <font>
                  <Font name="Courier New Bold" size="45.0" />
               </font>
            </Text>
            <Button mnemonicParsing="false" onMouseClicked="#goToRandomtxt" prefHeight="50.0" prefWidth="200.0" text="Random text">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets bottom="25.0" left="50.0" right="50.0" top="25.0" />
               </padding></Button>
            <Button mnemonicParsing="false" onMouseClicked="#goToRandomWords" prefHeight="50.0" prefWidth="200.0" text="Random words">
               <padding>
                  <Insets bottom="25.0" left="50.0" right="50.0" top="25.0" />
               </padding></Button>
            <Button mnemonicParsing="false" onMouseClicked="#goToChoosen" prefHeight="50.0" prefWidth="200.0" text="Choose text">
               <padding>
                  <Insets bottom="25.0" left="50.0" right="50.0" top="25.0" />
               </padding></Button>
            <Button alignment="TOP_CENTER" mnemonicParsing="false" onMouseClicked="#returnToMain" prefHeight="12.0" prefWidth="100.0" text="Return" textAlignment="CENTER">
               <padding>
                  <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
               </padding></Button>
         </children>
         <padding>
            <Insets top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
